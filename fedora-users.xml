<?xml version='1.0' ?>
  <users>
    <user name="fedoraAdmin" password="fedoraAdmin">
      <attribute name="fedoraRole">
        <value>administrator</value>
      </attribute>
    </user>
    <user name="fedoraReader" password="fedoraReader">
      <attribute name="fedoraRole">
        <value>reader</value>
      </attribute>
    </user>
