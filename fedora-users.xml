      <attribute name="fedoraRole">
        <value>anonymous</value>
      </attribute>
    </user>
    <user name="fedoraIntCallUser" password="changeme">
      <attribute name="fedoraRole">
        <value>fedoraInternalCall-1</value>
        <value>fedoraInternalCall-2</value>
      </attribute>
